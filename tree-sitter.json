{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "use",
      "camelcase": "Use",
      "title": "Use",
      "scope": "source.use",
      "file-types": [
        "use"
      ],
      "injection-regex": "^use$",
      "class-name": "TreeSitterUse",
      "highlights": "queries/highlights.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A tree-sitter parser for Gentoo USE-flags files",
    "authors": [
      {
        "name": "Vadim Komissarov",
        "email": "komissarov@eveloth.ru",
        "url": "https://errors.is/"
      }
    ],
    "links": {
      "repository": "https://github.com/eveloth/tree-sitter-use"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
